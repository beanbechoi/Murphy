unit CMurphyObj;interface	uses		TCL, GridPieceIntf;implementation	procedure CMurphyObj.KillMurphy;		var			mx, my: integer;			dorkyt: longint;	begin		CInfoApp(gapplication).GetMurphy.GetLoc(mx, my);		inherited Explode(mx, my);		CInfoApp(gapplication).GetGPane.GetBoard.SetData(mx, my, 145);		CInfoApp(gapplication).GetGPane.GetBPane.DrawOne(mx, my);		Delay(9, dorkyt);		CInfoApp(gApplication).GetMurphy.KillMurphy;	end;	procedure CMurphyObj.Destroy (x, y: integer);	begin		KillMurphy;	end;	procedure CMurphyObj.Explode (x, y: integer);	begin		KillMurphy;	end;end.