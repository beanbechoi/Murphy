unit CInfoChore;interface	uses		TCL, BackChoresIntf;implementation	procedure CInfoChore.IInfoChore (xe, ye: integer);	begin		xq := xe;		yq := ye;		myIdent := kInfoChore;	end;	procedure CInfoChore.GetChoreLoc (var cx, cy: integer);	begin		cx := xq;		cy := yq;	end;	function CInfoChore.Identify: integer;	begin		identify := myIdent mod 1000;	end;	function CInfoChore.ShouldRemove: boolean;		var			dat: integer;	begin		dat := cinfoapp(gapplication).getgpane.getboard.getdata(xq, yq);		ShouldRemove := (dat >= 600) and (dat <= 610);	end;	procedure CInfoChore.Remove;	begin		if myIdent < 1000 then			begin				CInfoApp(gApplication).FudRID(self);				myIdent := 1000 + myIdent;			end;	end;	procedure CInfoChore.FreeMemory;		{do nothing here; other objects might want to free themselves??}	begin	end;	procedure CInfoChore.Free;	begin		remove;	end;end.